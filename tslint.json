{
  "extends": "tslint:latest",
  "rules": {
    "class-name": true,
    "comment-format": [
      "check-lowercase"
    ],
    "component-class-suffix": true,
    "component-selector-name": [
      "camelCase",
      true
    ],
    "curly": true,
    "eofline": true,
    "forin": true,
    "indent": [
      true
    ],
    "interface-name": [
      "always-prefix",
      true
    ],
    "jsdoc-format": true,
    "no-any": false,
    "no-arg": true,
    "no-bitwise": false,
    "no-consecutive-blank-lines": true,
    "no-debugger": true,
    "no-console": [
      true
    ],
    "no-constructor-vars": false,
    "no-duplicate-key": true,
    "no-construct": false,
    "no-empty": true,
    "no-eval": true,
    "no-duplicate-variable": true,
    "no-internal-module": true,
    "no-require-imports": false,
    "no-trailing-comma": true,
    "no-trailing-whitespace": true,
    "no-unreachable": true,
    "no-unused-expression": true,
    "no-unused-variable": [
      true
    ],
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "object-literal-sort-keys": true,
    "quotemark": [
      "single"
    ],
    "semicolon": [
      "always",
      true
    ],
    "radix": true,
    "switch-default": true,
    "triple-equals": [
      true
    ],
    "use-strict": [
      true
    ],
    "variable-name": [
      true
    ]
  }
}
