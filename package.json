  },
  "homepage": "https://github.com/Stephn-R/super-angular2-starter#readme",
  "dependencies": {
    "autoprefixer": "6.4.0",
    "css-loader": "0.23.1",
    "ejs": "2.5.1",
    "eslint": "3.3.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "http-server": "0.9.0",
    "postcss-loader": "0.10.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
