{
  "name": "super-angular2-starter",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "lint": "eslint ./",
    "scan": "nsp check",
    "prebuild": "rimraf ./bin",
    "build": "webpack --progress --profile --colors --display-error-details",
    "build:dev": "export NODE_ENV=dev && npm run build",
    "build:prod": "export NODE_ENV=prod && npm run build",
    "prestart": "npm run build",
    "preserver": "npm run prebuild",
    "preprod": "npm run build:prod",
    "start": "http-server ./bin",
    "server": "webpack-dev-server --config ./config/webpack.server.js --content-base ./bin --progress --profile --colors --display-error-details --port 5555",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stephn-R/super-angular2-starter.git"
