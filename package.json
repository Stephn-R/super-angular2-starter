    "typescript": "1.8.10",
    "typings": "1.3.2",
    "webpack": "1.13.2",
    "webpack-dashboard": "0.1.7",
    "webpack-dev-server": "1.15.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "0.14.1",
    "webpack-notifier": "1.4.0"
  }
