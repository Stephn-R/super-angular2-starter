{
  "compilerOptions": {
    "preserveConstEnums": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "bin",
    "rootDir": ".",
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "bin",
    "config",
    "node_modules",
    "typings"
  ],
  "compileOnSave": false,
  "buildOnSave": false
}
