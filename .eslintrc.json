// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb",
  "globals": {
    "document": true,
    "window": true,
    "ENV": true
  },
  "rules": {
    "no-param-reassign": "off",
    "no-else-return": "off",
    "arrow-body-style": [1, "as-needed"]
  }
}
